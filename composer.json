{
  "name": "just-paja/defiant",
  "description": "Tiny PHP framework not meant for production use",
  "licesnse": "MIT",
  "authors": [
    {
      "name": "Pavel Žák",
      "email": "pavel@zak.global"
    }
  ],
  "autoload": {
    "exclude-from-classmap": ["/__tests__/"],
    "psr-4": {
      "Defiant\\": "lib"
    }
  },
  "archive": {
    "exclude": ["__tests__"]
  },
  "bin": ["bin/defiant"],
  "minimum-stability": "dev",
  "platform": {
    "php": "^7"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/just-paja/defiant-core"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "require": {
    "nategood/commando": "^0.2.9"
  }
}
